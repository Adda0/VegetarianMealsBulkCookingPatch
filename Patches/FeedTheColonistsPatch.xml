<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Patch>
<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Feed The Colonists</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="CookMealFineVegBulk"]/label</xpath>
					<value>
						<label>cook 4 vegetarian fine meals</label>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="CookMealLavishVegBulk"]/label</xpath>
					<value>
						<label>cook 4 vegetarian lavish meals</label>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="CookMealSurvivalVegBulk"]/label</xpath>
					<value>
						<label>cook 4 vegetarian packaged survival meals</label>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
